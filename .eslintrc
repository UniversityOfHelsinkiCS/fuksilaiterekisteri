{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "semi": [
      "error",
      "never"
    ],
    "max-len": [
      1,
      300
    ],
    "no-alert": [
      0
    ],
    "camelcase": 0,
    "no-plusplus": 0,
    "import/no-unresolved": "off",
    "react/jsx-filename-extension": "off",
    "class-methods-use-this": "off",
    "react/prop-types": [
      1,
      {
        "skipUndeclared": true
      }
    ]
  }
}